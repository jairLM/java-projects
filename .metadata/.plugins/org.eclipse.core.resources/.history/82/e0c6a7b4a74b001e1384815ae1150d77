package org.myProyect;

import java.util.ArrayList;

public class Deposito {
	
	int numCuenta;
	int monto;
	String tipo;
	
	
	
	{
	numCuenta = 00000;
	monto = 0;
	tipo = "Deposito";
	}
	
	Deposito(int numCuenta, int monto, String tipo){
		this.numCuenta = numCuenta;
		this.monto = monto;
		this.tipo = tipo;
	}
	
	
	
	
}
